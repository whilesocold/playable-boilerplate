{
    "name": "playable-boilerplate",
    "use3d": true,
    "version": "1.0.0",
    "description": "pixi.js typescript boilerplate webpack",
    "browserslist": "> 0.25%, not dead",
    "scripts": {
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- -w",
        "prebuild": "npm run clean",
        "clean": "npx rimraf dist",
        "build": "npm run type-check && npm run webpack -- --env mode=production",
        "start": "npm-run-all --parallel type-check:watch dev",
        "dev": "npm run webpack-dev-server -- --env mode=development",
        "prettier": "npx prettier",
        "prettier:format": "npm run prettier -- --write src/**/*.ts",
        "prettier:check": "npm run prettier -- -c ./src/**/*.ts",
        "lint-check": "npx eslint ./src/**/*.ts",
        "code-style-check": "npm run lint-check && npm run prettier:check"
    },
    "author": "whilesocold",
    "license": "MIT",
    "dependencies": {
        "@sflabs/pixi-joystick": "^6.0.3",
        "eventemitter3": "^4.0.7",
        "pixi.js": "^6.2.2",
        "remove": "^0.1.5",
        "three": "^0.142.0",
        "three-joystick": "^1.0.2"
    },
    "devDependencies": {
        "@swc/core": "^1.2.135",
        "@types/node": "^17.0.13",
        "@types/offscreencanvas": "^2019.6.4",
        "@types/three": "^0.141.0",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "browserslist": "^4.19.1",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-webpack-plugin": "^3.1.1",
        "html-webpack-plugin": "^5.5.0",
        "identity-obj-proxy": "^3.0.0",
        "jsdom": "^19.0.0",
        "mini-css-extract-plugin": "^2.5.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "regenerator-runtime": "^0.13.9",
        "rimraf": "^3.0.2",
        "swc-loader": "^0.1.15",
        "terser-webpack-plugin": "^5.3.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.3",
        "webpack-merge": "^5.8.0"
    }
}
